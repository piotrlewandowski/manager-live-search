{
  "name": "manager-live-search",
  "private": true,
  "version": "0.1.0",
  "description": "Manager Live Search",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --config webpack.config.dev.babel.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production --config webpack.config.prod.babel.js",
    "lint:js": "eslint --format friendly --ignore-path .gitignore --ext .js,.jsx src/",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint src/**/*.scss --syntax scss",
    "lint:scss:fix": "stylelint src/**/*.scss --syntax scss --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piotrlewandowski/manager-live-search.git"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "Piotr Lewandowski <contact@piotrlewandowski.info> (piotrlewandowski.info)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/piotrlewandowski/manager-live-search/issues"
  },
  "homepage": "https://github.com/piotrlewandowski/manager-live-search#readme",
